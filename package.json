{
  "name": "vue-env",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "build": "cross-env NODE_ENV=production webpack --profile --progress --hide-modules --config ./config/webpack.prod.js",
    "dev": "npm run prompt && cross-env NODE_ENV=development webpack-dev-server --open --profile --progress --config ./config/webpack.dev.js",
    "test:unit": "vue-cli-service test:unit",
    "precommit": "lint-staged",
    "prompt": "node ./config/prompt.config.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.8.3",
    "@cci-team/sass": "^0.0.5",
    "@vue/test-utils": "^1.0.0-beta.31",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-vue": "^6.1.2",
    "eslint-watch": "^7.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "inquirer": "^7.3.3",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "lint-staged": "^10.2.13",
    "mini-css-extract-plugin": "^0.11.0",
    "mocha": "^7.0.0",
    "node-sass": "^4.12.0",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.1.0",
    "shelljs": "^0.8.4",
    "url-loader": "^2.2.0",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node ./config/commit-lint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue,json}": [
      "esw --fix",
      "git add"
    ],
    "*.md": [
      "esw --ext '.md' --fix",
      "git add"
    ]
  }
}
